<hc:Window x:Class="GeometryX.MainWindow"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           xmlns:hc="https://handyorg.github.io/handycontrol" 
           xmlns:controls="clr-namespace:WpfControls.Controls;assembly=WpfControls"
           mc:Ignorable="d"
           Icon="icon.ico"
           Title="Geometry 资源图标" Height="450" Width="1200">
    <UniformGrid Rows="10" Columns="5">
        <controls:IconButton PathData="{DynamicResource IconAsk}" Content="IconAsk" Height="32" FontSize="18" Margin="2" HorizontalAlignment="Left"
                             Foreground="{DynamicResource AccentBrush}" MouseOverForeground="{DynamicResource PrimaryBrush}"/>
    </UniformGrid>
</hc:Window>